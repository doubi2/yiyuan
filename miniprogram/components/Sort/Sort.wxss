.doctorinfo_sort{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    width: 100%;
    height: 100rpx;
    border-bottom: solid #E7EAEF 1px;
    padding: 0 20rpx;
}
.sort_block{
    flex: 1;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 28rpx;
}
.sort_block image{
    width: 28rpx;
    height: 28rpx;
}