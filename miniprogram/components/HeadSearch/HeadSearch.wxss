.headsearch{
    background-color: #1769CC;
    width: 100%;
    height: 90rpx;
}
.searchframe{
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}
.search{
    display: flex;
    justify-content: center;
    align-items: center;
    width:90%;
    height: 56rpx;
    border-radius: 10rpx;
    background: #ffffff;
    font-size: 24rpx;
    margin: 7rpx 5%;
    color: #AEAEAE;
}
.search image{
    width: 28rpx;
    height: 28rpx;
    margin-right: 8rpx;
 }